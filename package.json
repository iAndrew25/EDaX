{
	"name": "edax",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "concurrently \"npm run client\" \"npm run server\"",
		"client": "vue-cli-service serve",
		"server": "nodemon server/server.js",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.16.4",
		"lodash": "^4.17.11",
		"vue": "^2.5.17",
		"vue-router": "^3.0.2",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.2.0",
		"@vue/cli-plugin-eslint": "^3.2.0",
		"@vue/cli-service": "^3.2.0",
		"babel-eslint": "^10.0.1",
		"concurrently": "^4.1.0",
		"eslint": "^5.8.0",
		"eslint-plugin-vue": "^5.0.0-0",
		"nodemon": "^1.18.9",
		"vue-template-compiler": "^2.5.17"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
